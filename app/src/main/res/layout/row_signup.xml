<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/linearRowgroupeleven"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="horizontal"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <FrameLayout android:id="@+id/frameUser"
        android:layout_height="17dp" android:layout_width="31dp">
        <com.google.android.material.switchmaterial.SwitchMaterial
            android:id="@+id/switchMaterialGroupEleven"
            android:layout_gravity="start"
            android:layout_height="wrap_content" android:layout_width="wrap_content"/>
        <View
            android:background="@drawable/rectangle_bg_red_901_radius_7"
            android:id="@+id/viewRectangle495"
            android:layout_gravity="center"
            android:layout_height="14dp"
            android:layout_marginHorizontal="1dp"
            android:layout_marginVertical="1dp" android:layout_width="28dp"/>
    </FrameLayout>
    <TextView android:ellipsize="end"
        android:id="@+id/txtHeledoyacep"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginStart="4dp"
        android:layout_weight="0.89" android:layout_width="0dp"
        android:singleLine="true"
        android:text="@{signupRowModel.txtHeledoyacep}"
        style="@style/txt_10" tools:text="He leído y acepto la Política de Privacidad"/>
</LinearLayout>