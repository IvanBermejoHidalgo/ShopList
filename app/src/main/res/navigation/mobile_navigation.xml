<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/logSignFragment">


    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.socialpuig.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_cambioContraFragment"
            app:destination="@id/cambioContraFragment" />
    </fragment>
    <fragment
        android:id="@+id/signOutFragment"
        android:name="com.example.socialpuig.SignOutFragment"
        android:label="fragment_sign_out"
        tools:layout="@layout/fragment_sign_out" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.socialpuig.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.socialpuig.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.socialpuig.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/newPostFragment"
        android:name="com.example.socialpuig.NewPostFragment"
        android:label="fragment_new_post"
        tools:layout="@layout/fragment_new_post" />
    <fragment
        android:id="@+id/mediaFragment"
        android:name="com.example.socialpuig.MediaFragment"
        android:label="fragment_media"
        tools:layout="@layout/fragment_media" />
    <fragment
        android:id="@+id/logSignFragment"
        android:name="com.example.socialpuig.LogSignFragment"
        android:label="fragment_log_sign"
        tools:layout="@layout/fragment_log_sign" >
        <action
            android:id="@+id/action_logSignFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
        <action
            android:id="@+id/action_logSignFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/cambioContraFragment"
        android:name="com.example.socialpuig.CambioContraFragment"
        android:label="fragment_cambio_contra"
        tools:layout="@layout/fragment_cambio_contra" >
        <action
            android:id="@+id/action_cambioContraFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/configuracionFragment"
        android:name="com.example.socialpuig.ConfiguracionFragment"
        android:label="fragment_configuracion"
        tools:layout="@layout/fragment_configuracion" />
    <fragment
        android:id="@+id/micuentaFragment"
        android:name="com.example.socialpuig.micuentaFragment"
        android:label="fragment_micuenta"
        tools:layout="@layout/fragment_micuenta" />
    <fragment
        android:id="@+id/ajustesFragment"
        android:name="com.example.socialpuig.ajustesFragment"
        android:label="fragment_ajustes"
        tools:layout="@layout/fragment_ajustes" />
    <fragment
        android:id="@+id/soporteFragment"
        android:name="com.example.socialpuig.soporteFragment"
        android:label="fragment_soporte"
        tools:layout="@layout/fragment_soporte" />
</navigation>